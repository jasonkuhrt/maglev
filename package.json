{
  "name": "maglev",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "imports": {
    "#lib/*": "./app/lib/*/$.js",
    "#core/*": "./app/core/*/$.js",
    "#*": "./app/*.js"
  },
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve build/server/index.js",
    "sandbox:init": "rm -rf sandbox/*.ts && cp sandbox/.templates/*.ts sandbox/",
    "sandbox:railway": "tsx sandbox/railway",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:routes": "react-router typegen",
    "gen:railway": "graffle",
    "fix": "pnpm run '/^fix:.*/'",
    "fix:format": "dprint fmt",
    "check": "pnpm run '/^check:.*/'",
    "check:format": "dprint check",
    "check:lint": "oxlint",
    "check:types": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@effect/platform": "^0.91.1",
    "@effect/platform-node": "^0.97.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@react-router/serve": "7.9.2",
    "@remix-run/node-fetch-server": "0.9.0",
    "@wollybeard/kit": "^0.63.0",
    "edgedb": "^2.0.1",
    "effect": "^3.17.14",
    "graffle": "8.0.0-next.168",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.2"
  },
  "devDependencies": {
    "@react-router/dev": "7.9.2",
    "@react-router/fs-routes": "7.9.2",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-rsc": "0.4.31",
    "dprint": "^0.50.2",
    "oxlint": "^1.18.0",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "5.1.4"
  }
}
