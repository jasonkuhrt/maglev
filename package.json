{
  "name": "maglev",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "imports": {
    "#lib/*": "./app/lib/*/$.js",
    "#core/*": "./app/core/*/$.js",
    "#*": "./app/*.js"
  },
  "scripts": {
    "build": "react-router build",
    "dev": "pnpm run --parallel '/^dev:.*/'",
    "dev:vite": "react-router dev",
    "dev:css": "panda --watch",
    "start": "react-router-serve build/server/index.js",
    "sandbox:init": "rm -rf sandbox/*.ts && cp sandbox/.templates/*.ts sandbox/",
    "sandbox:railway": "tsx sandbox/railway",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:css": "panda codegen",
    "gen:routes": "react-router typegen",
    "gen:railway": "graffle",
    "gen:gel": "gel generate js/edgeql-js --instance railway_interview --target esm --output-dir ./node_modules/@gel/client --force-overwrite && echo '{\"name\":\"@gel/client\",\"type\":\"module\",\"exports\":{\".\":{\"types\":\"./index.d.ts\",\"import\":\"./index.mjs\"}}}' > ./node_modules/@gel/client/package.json",
    "fix": "pnpm run '/^fix:.*/'",
    "fix:format": "dprint fmt",
    "check": "pnpm run '/^check:.*/'",
    "check:format": "dprint check",
    "check:lint": "oxlint",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/platform": "^0.91.1",
    "@effect/platform-node": "^0.97.1",
    "@react-router/serve": "7.9.3",
    "@remix-run/node-fetch-server": "0.9.0",
    "@wollybeard/kit": "^0.63.2",
    "edgedb": "^2.0.1",
    "effect": "^3.17.14",
    "gel": "^2.1.1",
    "graffle": "8.0.0-next.173",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "lucide-react": "^0.544.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.3"
  },
  "devDependencies": {
    "@gel/generate": "^0.6.4",
    "@pandacss/dev": "^1.4.0",
    "@react-router/dev": "7.9.3",
    "@react-router/fs-routes": "7.9.3",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "24.5.2",
    "@types/react": "19.1.14",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-rsc": "0.4.32",
    "dprint": "^0.50.2",
    "oxlint": "^1.18.0",
    "playwright": "^1.55.1",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.2.4"
  }
}
